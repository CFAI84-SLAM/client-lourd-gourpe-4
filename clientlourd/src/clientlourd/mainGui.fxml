<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clientlourd.MainGuiController">
   <children>
      <VBox layoutX="14.0" layoutY="62.0" prefHeight="191.0" prefWidth="72.0" spacing="15.0">
         <children>
            <Label text="ID">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label text="Photo">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label text="Nom">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label text="PrixHT">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label text="Categorie">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <VBox layoutX="86.0" layoutY="62.0" prefHeight="172.0" prefWidth="186.0" spacing="10.0">
         <children>
            <TextField fx:id="tfId" />
            <TextField fx:id="tfPhoto" />
            <TextField fx:id="tfNom" />
            <TextField fx:id="tfPrixHT" />
            <TextField fx:id="tfCategorie" />
         </children>
      </VBox>
      <TableView fx:id="tvProduits" layoutX="285.0" layoutY="43.0" onMouseClicked="#handleMouseAction" prefHeight="474.0" prefWidth="484.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="54.0" text="ID" />
          <TableColumn fx:id="colPhoto" prefWidth="146.0" text="Photo" />
            <TableColumn fx:id="colNom" prefWidth="134.0" text="Nom" />
            <TableColumn fx:id="colPrixHT" prefWidth="83.0" text="PrixHT" />
            <TableColumn fx:id="colCategorie" prefWidth="66.0" text="Catégorie" />
        </columns>
      </TableView>
      <Button fx:id="btnInserer" layoutX="14.0" layoutY="300.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="78.0" text="Insérer" />
      <Button fx:id="btnModifier" layoutX="101.0" layoutY="300.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="78.0" text="Modifier" />
      <Button fx:id="btnSupprimer" layoutX="187.0" layoutY="300.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Supprimer" />
   </children>
</AnchorPane>

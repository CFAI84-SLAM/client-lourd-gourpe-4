<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1057.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clientlourd.CommandesHistController">
   <children>
      <TableView fx:id="tvCommandes" layoutX="14.0" layoutY="84.0" onMouseClicked="#handleMouseAction" prefHeight="370.0" prefWidth="1027.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="54.0" text="ID" />
          <TableColumn fx:id="colEtat" prefWidth="207.0" text="État" />
            <TableColumn fx:id="colDate" prefWidth="274.0" text="Date de commande" />
            <TableColumn fx:id="colIdU" prefWidth="123.0" text="Id_Utilisateur" />
            <TableColumn fx:id="colEmail" prefWidth="368.0" text="Email" />
        </columns>
      </TableView>
      <Button fx:id="btnRetour" layoutX="54.0" layoutY="516.0" mnemonicParsing="false" onAction="#retour" prefHeight="38.0" prefWidth="77.0" text="Retour" />
      <Label layoutX="415.0" layoutY="28.0" prefHeight="27.0" prefWidth="226.0" text="Historique des commandes">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnMenu" layoutX="30.0" layoutY="15.0" mnemonicParsing="false" onAction="#switchToMenu" text="Menu" />
      <Button fx:id="btnDeco" layoutX="946.0" layoutY="16.0" mnemonicParsing="false" onAction="#disconnect" text="Déconnexion" />
   </children>
</AnchorPane>
